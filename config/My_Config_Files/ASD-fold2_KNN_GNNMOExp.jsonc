{
    "experiment" : {
        "scope": "examples_configs",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 2, "retrain": false}},
                {"in_sections" : ["do-pairs/oracle"],"params" : {"fold_id": -1,"retrain": false}},
                {"in_sections" : ["do-pairs/dataset"],"params" : {"compose_man" : "config/snippets/datasets/centr_and_weights.json"}}
           ]
        }
    },
    "do-pairs":[ 
        {
            "dataset": { 
                "class": "src.dataset.dataset_base.Dataset",
                "parameters": {
                    "generator": {
                        "class": "src.dataset.generators.asd.ASD", 
                        "parameters": { "data_dir": "data/datasets/autism/" }
                    }
                }
            },
               "oracle": {
        "class": "src.oracle.tabulars.knn.KNNOracle",           
        "parameters": {
            "embedder": {
                "class": "src.embedder.graph2vec.model.Graph2VecEmbedder", 
                "parameters": {
                    "model": {"parameters": { "wl_iterations": 17 } }
                }
            },
            "model": {  "parameters": {"n_neighbors":3} }         
                } 
            }
        }
        ],
    "explainers": [{"class": "src.explainer.generative.Project_Explainer_2.GNN_MOExp", "parameters": { "max_depth" : 3, "max_num_nodes" : 45 }}],
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
}